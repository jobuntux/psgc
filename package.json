{
  "name": "@jobuntux/psgc",
  "version": "0.2.1",
  "description": "Philippine Standard Geographic Code (PSGC) utilities and dataset for JavaScript/TypeScript projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jobuntux/psgc.git"
  },
  "homepage": "https://jobuntux.github.io/psgc/",
  "bugs": {
    "url": "https://github.com/jobuntux/psgc/issues"
  },
  "license": "MIT",
  "keywords": [
    "psgc",
    "philippines",
    "geodata",
    "regions",
    "provinces",
    "barangays"
  ],
  "author": "jobuntux <jobuntux@gmail.com>",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "data"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build:data": "tsx tools/build-data.ts",
    "prepare": "husky",
    "format": "prettier --write \"{src,tests,tools}/**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"{src,tests,tools}/**/*.{ts,tsx,js,json,md}\"",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "build": "tsc && tsc-alias",
    "log:clear": "node -e \"require('fs').writeFileSync('logs/development.log', '')\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "csv-parser": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitepress": "^1.6.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
